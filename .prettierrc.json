{
	"$schema": "https://json.schemastore.org/prettierrc",
	"arrowParens": "always",
	"semi": false,
	"tabWidth": 2,
	"useTabs": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"printWidth": 100,
	"trailingComma": "none",
	"bracketSpacing": true,
	"bracketSameLine": true,
	"parser": "vue",
	"vueIndentScriptAndStyle": true,
	"endOfLine": "lf",
	"insertFinalNewLine": "true",
	"spaceBeforeFunctionParen": "always",
	"singleAttributePerLine": false,
	"htmlWhitespaceSensitivity": "css",
	"proseWrap": "preserve",
	"insertPragma": false,
	"requirePragma": false,
	"embeddedLanguageFormatting": "auto",
	"spaceInParens": false,
	"json": {
		"tabWidth": 4
	}
}